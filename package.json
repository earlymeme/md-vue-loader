    "vue",
    "webpack",
    "loader"
  ],
  "dependencies": {
    "hash-sum": "^1.0.2",
    "highlight.js": "^9.12.0",
    "loader-utils": "^1.1.0",
    "markdown-it": "^8.4.0"
  },
  "devDependencies": {
    "css-loader": "^0.28.7",
    "eslint": "^4.19.1",
    "eslint-config-vusion": "^2.0.3",
    "markdown-it-for-inline": "^0.1.1",
    "markdown-it-task-lists": "^2.1.1",
    "raw-loader": "^0.5.1",
    "vue": "^2.5.2",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
